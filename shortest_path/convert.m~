num_diff = 0;

for i = 1:length(path)-1
    word1 = path(i);
    word2 = path(i+1);
    word1 = word1{1};
    word2 = word2{1};
    
    for j = 1:length(word1)
        if (word1(j) ~= word2(j))
           if (num_diff == 0)
               
           elseif (num_diff == 1)
               
           end 
        end
    end
end

end