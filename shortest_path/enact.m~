function enact(updown)

for i = 1:length(updown)
    
    row = updown(i,:);
    
    % Move to pos1
    %   Delay
    % Turn on magnet
    %   Delay
    % Move to pos2
    %   Delay
    % Turn off magnet
    %   Delay
    
    set_param('Final_Project_Controller/DC', 'value', row{1});
    pause(2);
    
    set_param('Final_Project_Controller/Mag', 'value', row{1});

end